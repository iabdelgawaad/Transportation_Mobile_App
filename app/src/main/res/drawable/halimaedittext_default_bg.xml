<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item  android:state_pressed="true" >
        <shape>
            <solid
                android:color="#FFA000"
                android:text="#1d0f29"/>
            <stroke
                android:width="2dp"
                android:color="#FFC107"
                android:text="#1d0f29"/>
            <corners
                android:radius="10dp" />

        </shape>
    </item>
    <item>
        <shape>
            <solid
                android:color="@color/abc_background_cache_hint_selector_material_light"
                android:text="#f8f9fa"/>
            <stroke
                android:width="2dp"
                android:color="#f4f4f4"
                android:text="#f4f4f4" />
            <corners
                android:radius="10dp" />
        </shape>
    </item>
</selector>
