<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item  android:state_pressed="true" >
        <shape>
            <solid
                android:color="#00796B"
                android:text="#1d0f29"/>
            <stroke
                android:width="1dp"
                android:color="#009688"
                android:text="#1d0f29"/>
            <corners
                android:radius="0dp" />

        </shape>
    </item>
    <item>
        <shape>
            <solid android:text="#f8f9fa"/>
            <stroke
                android:width="1dp"
                android:color="#f8f9fa"
                android:text="#ffffff" />
            <corners
                android:radius="0dp" />

        </shape>
    </item>
</selector>
